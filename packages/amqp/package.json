{
    "name": "@message-queue-toolkit/amqp",
    "version": "21.0.0",
    "private": false,
    "license": "MIT",
    "description": "AMQP adapter for message-queue-toolkit",
    "maintainers": [
        {
            "name": "Igor Savin",
            "email": "kibertoad@gmail.com"
        }
    ],
    "type": "module",
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "npm run clean && tsc --project tsconfig.build.json",
        "clean": "rimraf dist",
        "test": "vitest",
        "test:coverage": "npm run test -- --coverage",
        "lint": "biome check . && tsc",
        "lint:fix": "biome check --write .",
        "docker:start": "docker compose up -d rabbitmq",
        "docker:stop": "docker compose down",
        "prepublishOnly": "npm run lint && npm run build"
    },
    "dependencies": {
        "@lokalise/node-core": "^13.6.0",
        "zod": "^3.24.2"
    },
    "peerDependencies": {
        "@message-queue-toolkit/core": ">=21.1.0",
        "@message-queue-toolkit/schemas": ">=6.0.0",
        "amqplib": "^0.10.8"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@lokalise/biome-config": "^2.0.0",
        "@lokalise/tsconfig": "^1.3.0",
        "@message-queue-toolkit/core": "*",
        "@types/amqplib": "0.10.7",
        "@types/node": "^22.7.5",
        "@vitest/coverage-v8": "^3.1.3",
        "amqplib": "^0.10.8",
        "awilix": "^12.0.5",
        "awilix-manager": "^6.1.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.8.2",
        "vitest": "^3.1.3"
